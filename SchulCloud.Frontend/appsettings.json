{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MudBlazor.KeyInterceptorService": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "User": {
      "RequireUniqueEmail": true
    },
    "SignIn": {
      "RequireConfirmedEmail": true,
      "RequireConfirmedAccount": true
    },
    "EmailSender": {
      "DisplayedName": "SchulCloud",
      "Email": "no-reply@schulcloud"
    },
    "Fido2": {
      "AttestationConveyancePreference": "Direct",
      "UserVerificationRequirement": "Preferred"
    },
    "Tokens": {
      "AuthenticatorIssuer": "SchulCloud"
    },
    "TokenProviders": {
      "DataProtectionTokenProvider": {
        "TokenLifespan": "1:0:0"
      }
    },
    "ApiKeys": {
      "KeyPrefix": "sc",
      "GlobalSalt": "<A salt to secure the api keys>"
    }
  },
  "Fido2": {
    "ServerName": "SchulCloud"
  },
  "Api": {
    "DocumentationLinks": {
      "REST API": "/api/rest/swagger/index.html"
    }
  },
  "RequestLocalization": {
    "ApplyCurrentCultureToResponseHeaders": true,
    "DefaultRequestCulture": {
      "Culture": "en",
      "UICulture": "en"
    },
    "SupportedCultures": [ "en", "de" ],
    "SupportedUICultures": [ "en", "de" ]
  },
  "Mud": {
    "Snackbar": {
      "NewestOnTop": true,
      "PreventDuplicates": false
    }
  },
  "GoogleMaps": {
    "ApiKey": "<Your Apikey>"
  },
  "Presentation": {
    "ApplicationName": "SchulCloud",
    "Favicons": [
      {
        "Path": "/favicon.svg",
        "MimeType": "image/svg",
        "Sizes": "any"
      }
    ]
  }
}
