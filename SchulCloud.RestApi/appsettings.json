{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Identity.UserManager": "Information"
    }
  },
  "AllowedHosts": "*",
  "BasePath": "/api/rest",
  "Swagger": {
    "Enabled": true,
    "UiEnabled": true
  },
  "Commands": {
    "RateLimiter": {
      "Window": "0:0:10",
      "PermitLimit": 5
    }
  }
}
