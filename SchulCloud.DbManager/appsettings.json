{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Initializer": {
    "DefaultUser": {
      "Username": "<name>",
      "Email": "<email>",
      "Password": "<password>"
    }
  },
  "Cleaner": {
    "Interval": "0:30:0",
    "Timeout": "0:1:0",
    "MaxLoginAttemptsPerUser": 1000
  },
  "Commands": {
    "RateLimiter": {
      "Window": "0:0:10",
      "PermitLimit": 5
    }
  }
}
